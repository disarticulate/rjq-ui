{
  "name": "rjq-ui",
  "version": "1.5.0",
  "description": "RethinkDB Job Queue insights",
  "author": "Sagiv Frankel<sagivf@gmail.com>",
  "license": "MIT",
  "homepage": "https://sagivf.github.io/rjq-ui/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sagivf/rjq-ui.git"
  },
  "keywords": [
    "rethinkdb"
  ],
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  },
  "standard": {
    "ignore": "cert"
  },
  "bugs": {
    "url": "https://github.com/sagivf/rjq-ui/issues"
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "ghp": "node build/ghp.js",
    "start": "node ./bin/www",
    "start:nanobox": "node ./bin/www --db-host=$DATA_DB_HOST",
    "start:heroku": "node ./bin/www --db-host=$RETHINK_HOST --db-user=$RETHINK_USER --db-password=$RETHINK_PASSWORD --db-port=$RETHINK_PORT --db-cert=$RETHINK_CERT",
    "lint": "eslint --ext .js,.vue src",
    "upgrade": "npm run upgrade:rm && npm run upgrade:ncu && npm run upgrade:npm-install",
    "upgrade:rm": "rm node_modules -Rf",
    "upgrade:ncu": "npm-check-updates --upgradeAll",
    "upgrade:npm-install": "npm install"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "foundation-sites": "~6.3.1",
    "jquery": "^3.2.1",
    "koa": "^2.3.0",
    "koa-mount": "^3.0.0",
    "koa-static": "^4.0.1",
    "moment": "^2.18.1",
    "motion-ui": "^1.2.3",
    "path": "^0.12.7",
    "rjq-api-express": "^2.0.1",
    "rjq-api-koa": "^2.0.1",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "what-input": "^4.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chalk": "^2.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.28.4",
    "element-ui": "^1.4.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "leonardojs": "^3.1.12",
    "node-sass": "^4.5.3",
    "npm-check-updates": "^2.12.1",
    "opn": "^5.1.0",
    "ora": "^1.3.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.0",
    "semver": "^5.4.1",
    "shelljs": "^0.7.8",
    "standard": "^10.0.3",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-resource": "^1.3.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
